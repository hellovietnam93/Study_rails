<div class="box box-primary box-solid">
  <div class="box-header with-border">
    <h3 class="box-title">
      <%= t "class_rooms.members.all" %>
    </h3>
  </div>

  <div class="box-body">
    <table class="table table-border">
      <tbody>
        <% @members.each do |member| %>
          <tr>
            <td>
              <%= link_to member.user.email, member.user %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
