<tr class="class_room-<%= class_room.id %> trow" >
  <td class="uid">
    <%= link_to class_room.uid, [class_room] %>
  </td>
  <td class="name"><%= class_room.name %></td>
  <td class="description">
    <%= class_room.description %>
  </td>
  <td class="status"><%= class_room.status %></td>
  <td class="class_type"><%= class_room.class_type %></td>
  <td class="semester"><%= class_room.semester.name %></td>
  <td class="course"><%= class_room.course.uid %></td>
  <td class="max_student"><%= class_room.max_student %></td>
  <td class="registered_student"><%= class_room.registered_student %></td>
</tr>
