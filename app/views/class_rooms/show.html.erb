<% if show_login_class? current_user, @class_room %>
  <%= render "class_rooms/sign_in_class_modal" %>
<% else %>
  <% if current_user.lecturer? %>
    <%= render "nav_tabs" %>
  <% else %>
    <%= render "students/class_rooms/nav_tabs" %>
  <% end %>

  <%= render "assignment_submits/submit_assignment_modal" if current_user.student? %>
<% end %>
