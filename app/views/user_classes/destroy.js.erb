<% if @user_class.destroyed? %>
  $(".class_room-<%= @class_room.id %> .registered_student").html("<%= @class_room.registered_student %>")
  $(".student-<%= @user_class.id %>").remove()
  <% if current_user.student? %>
    $(".user-class").html("<%= j render 'class_rooms/login_class' %>")
    $(".btn-login-class").show();
    $("form.new_user_class")[0].reset();
    $(".new_user_class").hide();
  <% end %>
<% end %>
