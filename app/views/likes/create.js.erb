<% if @target.class.table_name == "comments" %>
  $("#comment-<%= @target.id %>").find(".comment-like").
    html("<%= j render 'likes/dislikes', like: @like %>")
<% elsif @target.class.table_name == "posts" %>
  $("#post-<%= @target.id %>").find(".post-like").
    html("<%= j render 'likes/dislikes', like: @like %>")
<% end %>
