$("#class_rooms #error_explanation").remove()

<% if @class_room.errors.any? %>
  $(".edit_class_room #errors-create-class_room").
    html("<%= j render 'shared/errors_messages', object: @class_room %>");
<% else %>
  $("#class_rooms .tbody").html("<%= j render 'admin/class_rooms/class_room',
    class_rooms: @class_rooms %>");
  $("form.edit_class_room")[0].reset();
  $("#new-class_room").hide();
  $(".btn-new-class_room").show();
<% end %>
set_timetout()
