$("#error_explanation").remove()
<% if @assignment_submit.errors.any? %>
  $("#errors-create-assignment-submit").
    html("<%= j render "shared/errors_messages", object: @assignment_submit %>")
<% else %>
  $("#submit-assignment-modal").modal("hide");
  $("iframe").contents().find("body").empty();
  $("form#new_assignment_submit")[0].reset();
  $("#all-submit-assignment-<%= @assignment_submit.assignment.id %> #assignment_submits .tbody").
    html("<%= j render @assignment_submits %>")
<% end %>
set_timetout()
